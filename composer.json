{
	"name": "beebot/activerecord",
	"description": "Generic ActiveRecord model implementation to detach data manipulation from the used database (NoSQL, Graph, Relational...)",
	"license": "proprietary",
	"authors": [
		{
			"name": "Stephane HULARD",
			"email": "s.hulard@chstudio.fr"
		}
	],
	"config": {
		"bin-dir": "bin"
	},
	"autoload": {
		"psr-4": {
			"BeeBot\\Entity\\": "src"
		}
	},
	"repositories": [
		{
			"type": "vcs",
			"url":  "https://bitbucket.org/beebot/events.git"
		}
	],
	"require": {
		"php": ">=5.4.0",
		"ext-spl": "*",
		"ext-pcre": "*",
		"ext-json": "*",
		"beebot/events": "~0",
		"bee4/transport": "~1"
	},
	"require-dev": {
		"phpunit/phpunit": "4.*",
		"behat/behat": "3.*",
		"sebastian/phpcpd": "~2",
		"pdepend/pdepend": "~2",
		"phpmd/phpmd": "~2",
		"phploc/phploc": "*",
		"squizlabs/php_codesniffer": "1.*"
	}
}
